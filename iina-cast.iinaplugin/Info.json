{
  "name": "IINA Cast",
  "identifier": "com.github.paulthery.iina-cast",
  "version": "1.0.0",
  "minIINAVersion": "1.4.0",
  "author": {
    "name": "Paul Thery",
    "url": "https://github.com/paulthery/IINA-Cast-Plugin"
  },
  "description": "Cast videos to Chromecast, DLNA/UPNP and AirPlay devices with full quality control and transcoding support",
  "entry": "main.js",
  "globalEntry": "global.js",
  "preferencePage": "preferences.html",
  "sidebarTab": {
    "name": "Cast"
  },
  "permissions": [
    "show-osd",
    "video-overlay",
    "network-request",
    "file-system"
  ],
  "preferenceDefaults": {
    "autoDiscovery": true,
    "preferredProtocol": "auto",
    "defaultMode": "direct",
    "transcodeBitrate": 20000,
    "transcodeResolution": "source",
    "audioFallback": "aac",
    "subtitleMode": "auto",
    "bufferDuration": 60,
    "prebufferEnabled": true,
    "helperPort": 9876
  }
}
